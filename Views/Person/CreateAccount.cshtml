        <!--begin: Wizard Step 2-->
        <div class="pb-5 card" data-wizard-type="step-content" id="address">
	        <div class="card-header">
		        <h4 class="mb-10 font-weight-bold text-dark">Your Address Details</h4>
	        </div>
	        <div class="card-body">
		        <div class="row col-md-12">
			        <div class="form-group col-md-4">
				        <b>House No</b>
				        <input asp-for="Address.HouseNo" class="form-control" placeholder="Enter House No"/>
				        <span asp-validation-for="Address.HouseNo" class="text-danger"></span>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Street Name</b>
				        <input asp-for="Address.StreetName" class="form-control" placeholder="Enter Street Name"/>
				        <span asp-validation-for="Address.StreetName" class="text-danger"></span>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Country</b><br>
				        <select asp-for="Address.CountryId" class="form-control select2-search--dropdown" id="CountryId" style="width: 100%" asp-items="ViewBag.CountryId">
					        <option value="" selected disabled hidden>Choose Your Country</option>
				        </select>
			        </div>
		        </div>

		        <div class="row col-md-12">
			        <div class="form-group col-md-4">
				        <b>Province</b><br>
				        <select asp-for="Address.ProvinceId" class="form-control select2-search--dropdown" id="ProvinceId" style="width: 100%" asp-items="ViewBag.ProvinceId">
					        <option value="" selected disabled hidden>Choose Your Province</option>
				        </select>
			        </div>
			        <div class="form-group col-md-4">
				        <b>City</b>
				        <select asp-for="Address.CityId" class="form-control select2-search--dropdown" id="CityId" style="width: 100%" asp-items="ViewBag.CityId">
					        <option value="" selected disabled hidden>Choose Your City</option>
				        </select>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Suburb</b>
				        <select asp-for="Address.SuburbId" class="form-control select2-search--dropdown" id="SuburbId" style="width: 100%" asp-items="ViewBag.SuburbId">
					        <option value="" selected disabled hidden>Choose Your Suburb</option>
				        </select>
			        </div>
		        </div>
		        <hr>
		        <div class="row col-md-12">
			        <div class="form-group col-md-4">
				        <b>Address Type</b>
				        <select asp-for="Address.AddressTypeId" class="form-control" asp-items="ViewBag.AddressTypeId">
					        <option value="" selected disabled hidden>Choose Address Type</option>
				        </select>
			        </div>
			        <div class="form-group col-md-4">
				       <b>PostalCode</b> 
				        <input asp-for="Address.PostalCode" class="form-control" placeholder="Enter Postal Code"/>
				        <span asp-validation-for="Address.PostalCode" class="text-danger"></span>
			        </div>
		        </div>
	        </div>
        </div>
