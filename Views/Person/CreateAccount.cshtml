        <!--begin: Wizard Form-->
        <form asp-action="CreateAccount" novalidate="novalidate" enctype="multipart/form-data" class="form pt-10 fv-plugins-bootstrap fv-plugins-framework" id="kt_form">
        <!--begin: Wizard Step 1-->
        <div class="pb-5 card" data-wizard-type="step-content" id="account">
	        <div class="card-header">
		        <h4 class="mb-10 font-weight-bold text-dark">Setup Account Details</h4>
	        </div>
	        <div class="card-body">
		        <div class="col-md-6">
			        <div class="image-input image-input-outline" id="kt_image_1">
				        <div class="image-input-wrapper" style="background-image: url(assets/media/users/default-avatar.png)"></div>

				        <label class="btn btn-xs btn-icon btn-circle btn-white btn-hover-text-primary btn-shadow" data-action="change" data-toggle="tooltip" title="" data-original-title="Change avatar">
					        <i class="fa fa-pen icon-sm text-muted"></i>
					        <input asp-for="Photo" type="file" name="Photo" class="form-group" accept="Image/*"/>
					        <input type="hidden" name="profile_avatar_remove"/>
				        </label>

				        <span class="btn btn-xs btn-icon btn-circle btn-white btn-hover-text-primary btn-shadow" data-action="cancel" data-toggle="tooltip" title="Cancel avatar">
					        <i class="ki ki-bold-close icon-xs text-muted"></i>
				        </span>
			        </div>
		        </div> 
		        <div class="row col-md-12">
			        <div class="form-group col-md-4">
				        <b>First Name</b>
				        <input asp-for="Person.FirstName" class="form-control"/>
				        <span asp-validation-for="Person.FirstName" class="text-danger"></span>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Last Name</b>
				        <input asp-for="Person.LastName" class="form-control"/>
				        <span asp-validation-for="Person.LastName" class="text-danger"></span>
			        </div>
			        <div class="form-group col-md-4"> 
				        <b>National ID</b>
				        <input asp-for="Person.NationalId" class="form-control"/>
				        <span asp-validation-for="Person.NationalId" class="text-danger"></span>
			        </div>
		        </div>

		        <div class="row col-md-12">
			        <div class="form-group col-md-4">
				        <b>Date of Birth</b>
				        <input asp-for="Person.PersonsDob" class="form-control"/>
				        <span asp-validation-for="Person.PersonsDob" class="text-danger"></span>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Email</b>
				        <input asp-for="Person.Email" class="form-control" disabled/>
				        <span asp-validation-for="Person.Email" class="text-danger"></span>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Phone Number</b>
				        <input asp-for="Person.PhoneNumber" class="form-control"/>
				        <span asp-validation-for="Person.PhoneNumber" class="text-danger"></span>
			        </div>
		        </div>
		        <hr>

		        <div class="row col-md-12">
			        <div class="form-group col-md-4">
				       <b>Gender</b>
				        <select asp-for="Person.GenderId" class="form-control " asp-items="ViewBag.GenderId">
					        <option value="" selected disabled hidden>Choose Your Gender</option>
				        </select>
			        </div>
			        <div class="form-group col-md-4">
				       <b>Equity</b>
				        <select asp-for="Person.EquityId" class="form-control " asp-items="ViewBag.EquityId">
					        <option value="" selected disabled hidden>Choose Equity</option>
				        </select>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Home Language</b>
				        <select asp-for="Person.HomeLanguageId" class="form-control r" asp-items="ViewBag.HomeLanguageId">
					        <option value="" selected disabled hidden>Choose Your Home Language</option>
				        </select>
			        </div>
		        </div>
		        <div class="row col-md-12">
			        <div class="form-group col-md-4">
				        <b>Nationality</b>
				        <select asp-for="Person.NationalityId" class="form-control " asp-items="ViewBag.NationalityId">
					        <option value="" selected disabled hidden>Choose Nationality</option>
				        </select>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Citizenship Status</b>
				        <select asp-for="Person.CitizenshipStatusId" class="form-control " asp-items="ViewBag.CitizenshipStatusId">
					        <option value="" selected disabled hidden>Choose Your Citizenship Status</option>
				        </select>
			        </div>
			        <div class="form-group col-md-4">
				        <b>Disability Status</b>
				        <select asp-for="Person.DisabilityStatusId" class="form-control " asp-items="ViewBag.DisabilityStatusId">
					        <option value="" selected disabled hidden>Choose Disability Status</option>
				        </select>
			        </div>
		        </div>
		        @*<div class="form-group row ml-5">
