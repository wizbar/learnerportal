<!--begin::Card-->
<div class="card card-custom gutter-b mt-6">
	<div class="card-header">
		<div class="card-title">
			<h3 class="card-label">Job Applications for <strong>@Html.DisplayFor(model => model.FirstName) @Html.DisplayFor(model => model.LastName)</strong></h3>
		</div>
	</div>
	<div class="card-body">
		<!--begin::Example-->
		<div class="example mb-10">
			<div class="example-preview">
				<table class="table table-sm">
					<thead>
					<tr>
						<th scope="col">Job Title</th>
						<th scope="col">Date Applied</th>
						<th scope="col">Expiry Date</th> 
						<th scope="col">Application Status</th>
						<th scope="col">Province for Application</th>
					</tr>
					</thead>
				
					<tbody>
					@foreach(var job in Model.Applications){
						<tr>
							<td>@Html.DisplayFor(m => job.JobTitle)</td>
							<td>@Html.DisplayFor(m => job.DateApplied)</td> 
							<td>@Html.DisplayFor(m => job.ExpiryDate)</td>
							<td>
								<span class="label label-inline label-light-primary font-weight-bold">@Html.DisplayFor(m => job.JobApplicationStatus)</span>
							</td>
							<td>@Html.DisplayFor(model => model.ProvinceName)</td>
						</tr>
					}
					</tbody>
				</table>
				
			</div>
		</div>
		<!--end::Example-->
	</div>
</div>
<!--end::Card-->
</div>
<!--end::Container-->
</div>
