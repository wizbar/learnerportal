@model learner_portal.Models.Sector

                <form asp-action="Details">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div id="progressbarwizard">

                        <div class="tab-content mb-0 b-0">

                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <b>@Html.DisplayNameFor(model => model.SectorDesc):</b>
                                        <div class="form-control form-control-feedback h-auto p-3">
                                            @Html.DisplayFor(model => model.SectorDesc)
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <b>@Html.DisplayNameFor(model => model.ActiveYn):</b>
                                        <div class="form-control form-control-feedback h-auto p-3">
                                            @Html.DisplayFor(model => model.ActiveYn)
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <a asp-action="Edit" class="btn btn-success mr-1" asp-route-id="@Model.SectorId">Edit</a>
                    </div>
                </form>